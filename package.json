{
  "name": "engbook",
  "version": "0.3.0",
  "description": "blog application using react, redux, express, mongodb",
  "main": "index.js",
  "scripts": {
    "start": "cd server && npm start",
    "install": "cocurrently \"cd server && yarn install\" \"cd client && yarn install\"",
    "dev": "concurrently \"npm run client \" \"npm run server\"",
    "server": "cd server && npm run dev",
    "server:dev": "cd server && npm run dev",
    "test": "cd server && npm test",
    "test:dev:server": "cd server && npm run test dev",
    "client": "cd client && yarn start",
    "tclient": "cd client && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escobard/engbook.git"
  },
  "author": "Daniel Escobar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/escobard/engbook/issues"
  },
  "homepage": "https://github.com/escobard/engbook#readme",
  "dependencies": {
    "concurrently": "^3.5.1"
  },
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "devDependencies": {
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "react-test-renderer": "^16.2.0"
  }
}
